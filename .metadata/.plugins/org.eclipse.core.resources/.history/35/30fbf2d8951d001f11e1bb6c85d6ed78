package Coursera;

import org.openqa.selenium.WebDriver;
import org.openqa.selenium.support.PageFactory;

public class BaseClassForReusableContructor {

	WebDriver driver;
	
	public BaseClassForReusableContructor(WebDriver driver)
	{
		this.driver=driver;
		PageFactory.initElements(driver,this);
	}
	
}
